{ parameter nat ;
  storage (pair (nat %votingPeriodIndex) (address %admin)) ;
  code { UNPAIR ;
         DUP 2 ;
         CDR ;
         SENDER ;
         COMPARE ;
         NEQ ;
         IF { DROP 2 ; PUSH string "You are not the admin !!!" ; FAILWITH }
            { UPDATE 1 ; NIL operation ; PAIR } } ;
  view "currentVotingPeriod" unit nat { CDR ; CAR } }

